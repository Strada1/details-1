{
	"env": {
		"browser": true,
		"es2021": true
	},

	"extends": "eslint:recommended",
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"no-debugger": "error",
		"no-console": "error",
		"camelcase": "error"
	},

	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	}
}
